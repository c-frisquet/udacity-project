{
  "parser": "babel-eslint",
  "extends": ["google"],
  "rules": {
    "indent": ["error", 4],
    "max-len": ["warn", 120],
    "no-undef": "off",
    "require-jsdoc": "off",
    "no-negated-condition": "off",
    "no-unused-vars": "off",
    "no-alert": "off"
  },
  "env": {
    "browser": true,
    "commonjs": true,
    "es6": true,
    "node": true
  }
}
